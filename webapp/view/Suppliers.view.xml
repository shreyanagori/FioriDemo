<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ns.businesspartners.controller.Suppliers" displayBlock="true">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
					    <List noDataText="Drop list items here" id="list1" items="{/BusinessPartnerSet}">
					        <items>
					            <StandardListItem type="Navigation" title="{CompanyName}" description="{WebAddress}" icon="sap-icon://picture" id="item1"/>
					        </items>
					    </List>					   
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>